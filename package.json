{
  "name": "edam-browser",
  "description": "The EDAM Browser is a client-side web-based visualization javascript widget. Its goals are to help describing bio-related resources and service with EDAM, and to facilitate and foster community contributions to EDAM.",
  "author": "Bryan Brancotte",
  "version": "1.0.0",
  "scripts": {
    "dev": "live-server",
    "test": "jshint js/*js",
    "predeploy": "rm -rf dist && parcel build ./*.html --public-url https://hagerdakroury.github.io/edam-browser/",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "jshint": "^2.9.5",
    "live-server": "^1.2.1"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "d3": "^4.13.0",
    "gh-pages": "^3.2.3",
    "jquery": "^3.2.1",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "jquery-ui-dist": "^1.12.1",
    "jquery-ui-themeroller": "^0.0.5",
    "jquery-ui-themes": "^1.12.0",
    "parcel": "^2.0.0-nightly.769",
    "popper.js": "^1.16.1",
    "regenerator-runtime": "^0.13.7"
  }
}
